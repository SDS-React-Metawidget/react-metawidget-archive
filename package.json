{
  "name": "react-metawidget",
  "version": "0.0.0",
  "description": "Metawidget for React",
  "main": "dist/index.js",
  "scripts": {
    "build": "webpack",
    "clean": "GLOBIGNORE=.gitignore:.npmignore rm -rf ./dist/*",
    "cover": "babel-node ./node_modules/.bin/isparta cover _mocha -- --reporter dot",
    "lint": "eslint ./src ./test",
    "lint:fix": "eslint --fix ./src ./test",
    "start": "npm run clean && npm run build",
    "test": "mocha",
    "watch": "npm run clean && webpack --watch"
  },
  "keywords": [
    "metawidget",
    "react"
  ],
  "author": "Richard Kennard",
  "contributors": [
    "Nathan Hardy (http://nhardy.id.au/)"
  ],
  "license": "LGPL-2.1",
  "devDependencies": {
    "app-module-path": "^1.0.6",
    "babel-cli": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-syntax-object-rest-spread": "^6.5.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "eslint": "^2.8.0",
    "eslint-config-nhardy": "^0.0.8",
    "mocha": "^2.4.5",
    "webpack": "^1.13.0",
    "webpack-node-externals": "^1.2.0"
  },
  "dependencies": {
    "lodash": "^4.11.1",
    "metawidget": "^4.2.0",
    "react": "^15.0.2"
  }
}
